{
    acme_dns cloudflare {env.CLOUDFLARE_API_TOKEN}
    
    dynamic_dns {
        provider cloudflare {env.CLOUDFLARE_API_TOKEN}
        domains {
            hoard.bowline.im
        }
        # Optional: check interval (default is 5m)
        check_interval 5m
    }
}

hoard.bowline.im {
  @internal {
        remote_ip private_ranges
  }
	reverse_proxy hoarder:5555
}

