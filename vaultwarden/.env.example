TZ=America/Mexico_City
SUBDOMAIN=vaultwarden
CONFIG_PATH=/path/to/config

# Generate with:
# docker exec -it vaultwarden /vaultwarden hash
ADMIN_TOKEN=

# Security settings
SIGNUPS_ALLOWED=false
LOG_LEVEL=warn

# Database configuration
POSTGRES_DB=vaultwarden
POSTGRES_USER=vaultwarden
POSTGRES_PASSWORD=

# Optional: Remove database section from docker-compose.yml to use SQLite instead
# For SQLite, comment out the DATABASE_URL environment variable in docker-compose.yml
